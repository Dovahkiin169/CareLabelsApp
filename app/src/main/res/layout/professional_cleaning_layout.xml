<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:background="?attr/care_label_drawable"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <androidx.appcompat.widget.AppCompatImageButton
        android:id="@+id/professional_wet_cleaning"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="4dp"
        android:background="?attr/button_transparent_icon_shape_drawable"
        android:paddingStart="1dp"
        android:paddingTop="1dp"
        android:paddingEnd="1dp"
        android:paddingBottom="1dp"
        android:scaleType="fitCenter"
        android:src="@drawable/professional_wet_cleaning_sizer"
        app:layout_constraintBottom_toTopOf="@+id/space"
        app:layout_constraintEnd_toStartOf="@+id/gentle_wet_cleaning"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.appcompat.widget.AppCompatImageButton
        android:id="@+id/dry_clean_hydrocarbon_solvent_only_hcs"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="4dp"
        android:background="?attr/button_transparent_icon_shape_drawable"
        android:paddingStart="1dp"
        android:paddingTop="1dp"
        android:paddingEnd="1dp"
        android:paddingBottom="1dp"
        android:scaleType="fitCenter"
        android:src="@drawable/dry_clean_hydrocarbon_solvent_only_hcs_sizer"
        app:layout_constraintBottom_toTopOf="@+id/space2"
        app:layout_constraintEnd_toStartOf="@+id/gentle_cleaning_with_hydrocarbon_solvents"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/space" />

    <androidx.appcompat.widget.AppCompatImageButton
        android:id="@+id/very_gentle_cleaning_with_pce"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="4dp"
        android:background="?attr/button_transparent_icon_shape_drawable"
        android:paddingStart="1dp"
        android:paddingTop="1dp"
        android:paddingEnd="1dp"
        android:paddingBottom="1dp"
        android:scaleType="fitCenter"
        android:src="@drawable/very_gentle_cleaning_with_pce_sizer"
        app:layout_constraintBottom_toTopOf="@+id/space3"
        app:layout_constraintEnd_toStartOf="@+id/do_not_dry_clean"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/space2" />

    <androidx.appcompat.widget.AppCompatImageButton
        android:id="@+id/do_not_dry_clean"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="4dp"
        android:background="?attr/button_transparent_icon_shape_drawable"
        android:paddingStart="1dp"
        android:paddingTop="1dp"
        android:paddingEnd="1dp"
        android:paddingBottom="1dp"
        android:scaleType="fitCenter"
        android:src="@drawable/do_not_dry_clean_sizer"
        app:layout_constraintBottom_toTopOf="@+id/space3"
        app:layout_constraintEnd_toEndOf="@+id/gentle_cleaning_with_hydrocarbon_solvents"
        app:layout_constraintStart_toEndOf="@+id/very_gentle_cleaning_with_pce"
        app:layout_constraintTop_toBottomOf="@+id/space2" />

    <androidx.appcompat.widget.AppCompatImageButton
        android:id="@+id/very_gentle_wet_cleaning"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="4dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="4dp"
        android:background="?attr/button_transparent_icon_shape_drawable"
        android:paddingStart="1dp"
        android:paddingTop="1dp"
        android:paddingEnd="1dp"
        android:paddingBottom="1dp"
        android:scaleType="fitCenter"
        android:src="@drawable/very_gentle_wet_cleaning_sizer"
        app:layout_constraintBottom_toTopOf="@+id/space"
        app:layout_constraintEnd_toStartOf="@+id/do_not_wet_clean"
        app:layout_constraintStart_toEndOf="@+id/gentle_wet_cleaning"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.appcompat.widget.AppCompatImageButton
        android:id="@+id/very_gentle_cleaning_with_hydrocarbon_solvents"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="4dp"
        android:layout_marginEnd="4dp"
        android:background="?attr/button_transparent_icon_shape_drawable"
        android:paddingStart="1dp"
        android:paddingTop="1dp"
        android:paddingEnd="1dp"
        android:paddingBottom="1dp"
        android:scaleType="fitCenter"
        android:src="@drawable/very_gentle_cleaning_with_hydrocarbon_solvents_sizer"
        app:layout_constraintBottom_toTopOf="@+id/space2"
        app:layout_constraintEnd_toStartOf="@+id/dry_clean_tetrachloroethylene_pce_only"
        app:layout_constraintStart_toEndOf="@+id/gentle_cleaning_with_hydrocarbon_solvents"
        app:layout_constraintTop_toBottomOf="@+id/space" />

    <androidx.appcompat.widget.AppCompatImageButton
        android:id="@+id/do_not_wet_clean"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="4dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="4dp"
        android:background="?attr/button_transparent_icon_shape_drawable"
        android:paddingStart="1dp"
        android:paddingTop="1dp"
        android:paddingEnd="1dp"
        android:paddingBottom="1dp"
        android:scaleType="fitCenter"
        android:src="@drawable/do_not_wet_clean_sizer"
        app:layout_constraintBottom_toTopOf="@+id/space"
        app:layout_constraintEnd_toStartOf="@+id/dry_clean_any_solvent"
        app:layout_constraintStart_toEndOf="@+id/very_gentle_wet_cleaning"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.appcompat.widget.AppCompatImageButton
        android:id="@+id/dry_clean_tetrachloroethylene_pce_only"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="4dp"
        android:layout_marginEnd="4dp"
        android:background="?attr/button_transparent_icon_shape_drawable"
        android:paddingStart="1dp"
        android:paddingTop="1dp"
        android:paddingEnd="1dp"
        android:paddingBottom="1dp"
        android:scaleType="fitCenter"
        android:src="@drawable/dry_clean_tetrachloroethylene_pce_only_sizer"
        app:layout_constraintBottom_toTopOf="@+id/space2"
        app:layout_constraintEnd_toStartOf="@+id/gentle_cleaning_with_pce"
        app:layout_constraintStart_toEndOf="@+id/very_gentle_cleaning_with_hydrocarbon_solvents"
        app:layout_constraintTop_toBottomOf="@+id/space" />

    <androidx.appcompat.widget.AppCompatImageButton
        android:id="@+id/dry_clean_any_solvent"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="4dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:background="?attr/button_transparent_icon_shape_drawable"
        android:paddingStart="1dp"
        android:paddingTop="1dp"
        android:paddingEnd="1dp"
        android:paddingBottom="1dp"
        android:scaleType="fitCenter"
        android:src="@drawable/dry_clean_any_solvent_sizer"
        app:layout_constraintBottom_toTopOf="@+id/space"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/do_not_wet_clean"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.appcompat.widget.AppCompatImageButton
        android:id="@+id/gentle_cleaning_with_pce"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="4dp"
        android:layout_marginEnd="8dp"
        android:background="?attr/button_transparent_icon_shape_drawable"
        android:paddingStart="1dp"
        android:paddingTop="1dp"
        android:paddingEnd="1dp"
        android:paddingBottom="1dp"
        android:scaleType="fitCenter"
        android:src="@drawable/gentle_cleaning_with_pce_sizer"
        app:layout_constraintBottom_toTopOf="@+id/space2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/dry_clean_tetrachloroethylene_pce_only"
        app:layout_constraintTop_toBottomOf="@+id/space" />

    <androidx.appcompat.widget.AppCompatImageButton
        android:id="@+id/gentle_wet_cleaning"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="4dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="4dp"
        android:background="?attr/button_transparent_icon_shape_drawable"
        android:paddingStart="1dp"
        android:paddingTop="1dp"
        android:paddingEnd="1dp"
        android:paddingBottom="1dp"
        android:scaleType="fitCenter"
        android:src="@drawable/gentle_wet_cleaning_sizer"
        app:layout_constraintBottom_toTopOf="@+id/space"
        app:layout_constraintEnd_toStartOf="@+id/very_gentle_wet_cleaning"
        app:layout_constraintStart_toEndOf="@+id/professional_wet_cleaning"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.appcompat.widget.AppCompatImageButton
        android:id="@+id/gentle_cleaning_with_hydrocarbon_solvents"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="4dp"
        android:layout_marginEnd="4dp"
        android:background="?attr/button_transparent_icon_shape_drawable"
        android:paddingStart="1dp"
        android:paddingTop="1dp"
        android:paddingEnd="1dp"
        android:paddingBottom="1dp"
        android:scaleType="fitCenter"
        android:src="@drawable/gentle_cleaning_with_hydrocarbon_solvents_sizer"
        app:layout_constraintBottom_toTopOf="@+id/space2"
        app:layout_constraintEnd_toStartOf="@+id/very_gentle_cleaning_with_hydrocarbon_solvents"
        app:layout_constraintStart_toEndOf="@+id/dry_clean_hydrocarbon_solvent_only_hcs"
        app:layout_constraintTop_toBottomOf="@+id/space" />

    <Space
        android:id="@+id/space"
        android:layout_width="0dp"
        android:layout_height="8dp"
        android:layout_marginTop="60dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Space
        android:id="@+id/space2"
        android:layout_width="0dp"
        android:layout_height="8dp"
        android:layout_marginTop="120dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Space
        android:id="@+id/space3"
        android:layout_width="0dp"
        android:layout_height="8dp"
        android:layout_marginTop="180dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>